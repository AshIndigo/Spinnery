{
  id: "spinnery:dark",
  name: "Dark Theme",
  description: "Mercy for your eyes with the default dark theme",
  authors: ["Spinnery Team"],
  vars: {
    bevel_dark: 0xff1b1b1b,
    bevel_light: 0xff6a6a6a,
    bg_panel: 0xff393939,
    bg_interactable: 0xff292929
  },
  prototypes: {
    panel: {
      shadow: 0xff262626,
      background: "$bg_panel",
      highlight: 0xff474747,
      outline: 0xff000000,
      label: 0xffffffff
    },
    slider: {
      top_left: {
        background: "$bevel_dark",
        foreground: "$bevel_light"
      },
      bottom_right: {
        background: "$bevel_light",
        foreground: "$bevel_dark"
      },
      background: {
        on: 0xff00c116,
        off: "$bg_interactable"
      },
      foreground: 0xff3a3a3a,
      text: 0xffffffff
    }
  },
  theme: {
    "spinnery:interface": {
      $extend: ["$panel"]
    },
    "spinnery:vertical_list": {
      $extend: ["$panel"]
    },
    "spinnery:horizontal_list": {
      $extend: ["$panel"]
    },
    "spinnery:draggable_area": {
      $extend: ["$panel"],
      area: {
        start: 0xff141414,
        end: 0xff202020
      },
    },
    "spinnery:dropdown": {
      $extend: ["$panel"]
    },
    "spinnery:static_image": {
    },
    "spinnery:dynamic_image": {
    },
    "spinnery:static_text": {
      text: 0xffffffff,
      shadow: 0xff3e3e3e
    },
    "spinnery:dynamic_text": {
      top_left: "$bevel_light",
      bottom_right: "$bevel_dark",
      background: 0xff000000,
      text: 0xffffffff,
      highlight: 0xb30400ff,
      cursor: 0xffffffff,
      label: 0xff808080
    },
    "spinnery:vertical_slider": {
      $extend: ["$slider"]
    },
    "spinnery:horizontal_slider": {
      $extend: ["$slider"]
    },
    "spinnery:toggle": {
      $extend: ["$slider"]
    },
    "spinnery:button": {
      top_left: {
        on: "$bevel_dark",
        off: "$bevel_light"
      },
      bottom_right: {
        on: "$bevel_light",
        off: "$bevel_dark"
      },
      background: {
        on: "$bevel_dark",
        off: "$bg_interactable"
      },
      label: 0xffffffff
    },
    "spinnery:slot": {
      top_left: "$bevel_dark",
      bottom_right: "$bevel_light",
      background: {
        focused: 0xffc5c5c5,
        unfocused: "$bg_interactable"
      }
    },
    "spinnery:tab_holder": {
      $extend: ["$panel"]
    },
    "spinnery:tab_toggle": {
      shadow: {
        on: 0xff545454,
        off: 0xff262626,
      },
      background: {
        on: 0xff444444,
        off: 0xff393939
      },
      highlight: {
        on: 0xff353535,
        off: 0xff474747
      },
      outline: {
        on: 0xff000000,
        off: 0xff000000,
      },
      label: 0xffffffff
    },
    "spinnery:tooltip": {
      background: {
        start: 0xf0100010,
        end: 0xf0100010,
      },
      outline: {
        start: 0x505000ff,
        end: 0x5028007f,
      },
      shadow: {
        start: 0xf0100010,
        end: 0xf0100010,
      }
    },
    "spinnery:bar": {
      background: "spinnery:textures/bar_background_dark.png",
      foreground: "spinnery:textures/bar_foreground_dark.png"
    },
    "spinnery:vertical_scrollbar": {
      scroller: {
        top_left: "$bevel_light",
        bottom_right: "$bevel_dark",
        background: "$bg_panel",
      },
      scroll_line: {
        top_left: "$bevel_dark",
        bottom_right: "$bevel_light",
        background: "$bg_interactable",
      },
    },
    "spinnery:horizontal_scrollbar": {
      scroller: {
        top_left: "$bevel_light",
        bottom_right: "$bevel_dark",
        background: "$bg_panel",
      },
      scroll_line: {
        top_left: "$bevel_dark",
        bottom_right: "$bevel_light",
        background: "$bg_interactable",
      }
    }
  }
}